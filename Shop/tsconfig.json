{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "lib": [ "es2015", "dom" ],
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "types": [ "webpack-env" ],
    "experimentalDecorators": true,
    "strictPropertyInitialization": false
  },
  "exclude": [
    "bin",
    "node_modules"
  ]
}
